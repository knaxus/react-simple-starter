{
  "name": "react-starter-v2",
  "version": "1.0.0",
  "main": "index.js",
  "author": "Ashok Dey <ashokdey100@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "webpack-dev-server --config=config/webpack.dev.js",
    "build": "npm run clear && node ./config/custom_scripts/copy_assets && webpack --config=config/webpack.prod.js",
    "quick": "npm run clean-init && npm install && npm start",
    "clear": "rm -rf build",
    "clean-init": "rm -rf .git _assets README.md && git init && git add . && git commit -m \"initial commit\"",
    "cont": "node ./config/custom_scripts/create_container",
    "comp": "node ./config/custom_scripts/create_component",
    "test": "jest --watch",
    "test:cover": "jest --coverage"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "fs-extra": "^8.1.0",
    "html-webpack-plugin": "^3.2.0",
    "identity-obj-proxy": "^3.0.0",
    "prop-types": "^15.7.2",
    "react": "^16.10.1",
    "react-dom": "^16.10.1",
    "react-redux": "^7.1.1",
    "react-redux-toastr": "^7.5.2",
    "react-router-dom": "^5.1.2",
    "redux": "^4.0.4",
    "redux-saga": "^1.1.1",
    "validator": "^11.1.0",
    "webpack": "4.41.0",
    "webpack-manifest-plugin": "^2.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/plugin-transform-modules-commonjs": "^7.0.0",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.4.2",
    "babel-loader": "^8.0.6",
    "babel-plugin-dynamic-import-node": "^2.3.0",
    "css-loader": "^3.2.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "enzyme-to-json": "^3.4.2",
    "eslint": "^6.5.1",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-loader": "^3.0.2",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.15.1",
    "file-loader": "^4.2.0",
    "jest-cli": "^24.9.0",
    "node-sass": "^4.12.0",
    "prettier-eslint": "^9.0.0",
    "react-addons-test-utils": "^15.6.2",
    "react-hot-loader": "4.12.14",
    "react-test-renderer": "^16.10.1",
    "sass-loader": "^8.0.0",
    "style-loader": "^1.0.0",
    "webpack-dev-server": "^3.8.2"
  },
  "jest": {
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    },
    "setupFiles": [
      "./config/custom_scripts/Test/jestSetup.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "moduleNameMapper": {
      "^.+\\.(css|scss)$": "identity-obj-proxy"
    }
  }
}
